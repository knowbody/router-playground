{
  "name": "router-playground",
  "version": "1.0.0",
  "description": "just playing with react router",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "test": "BABEL_JEST_STAGE=0 jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/history"
    ],
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knowbody/router-playground.git"
  },
  "author": "Mateusz Zatorski <mateuszzatorski@gmail.com> (https://github.com/knowbody)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knowbody/router-playground/issues"
  },
  "homepage": "https://github.com/knowbody/router-playground#readme",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.0.1",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^3.6.3",
    "express": "^4.13.3",
    "fs": "0.0.2",
    "jest-cli": "^0.7.1",
    "json-loader": "^0.5.3",
    "react-addons-test-utils": "^0.14.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "dependencies": {
    "history": "^1.13.0",
    "pixi.js": "^3.0.8",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-pixi": "^0.7.3",
    "react-router": "^1.0.0"
  }
}
